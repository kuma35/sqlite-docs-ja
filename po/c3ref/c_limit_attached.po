"#define SQLITE_LIMIT_EXPR_DEPTH                3\n"
"#define SQLITE_LIMIT_COMPOUND_SELECT           4\n"
"#define SQLITE_LIMIT_VDBE_OP                   5\n"
"#define SQLITE_LIMIT_FUNCTION_ARG              6\n"
"#define SQLITE_LIMIT_ATTACHED                  7\n"
