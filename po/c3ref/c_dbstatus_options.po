"#define SQLITE_DBSTATUS_STMT_USED            3\n"
"#define SQLITE_DBSTATUS_LOOKASIDE_HIT        4\n"
"#define SQLITE_DBSTATUS_LOOKASIDE_MISS_SIZE  5\n"
"#define SQLITE_DBSTATUS_LOOKASIDE_MISS_FULL  6\n"
