"  int(*xCompare)(void*,int,const void*,int,const void*)\n"
");\n"
"int sqlite3_create_collation_v2(\n"
"  sqlite3*,\n"
"  const char *zName,\n"
"  int eTextRep,\n"
"  void *pArg,\n"
"  int(*xCompare)(void*,int,const void*,int,const void*),\n"
"  void(*xDestroy)(void*)\n"
");\n"
"int sqlite3_create_collation16(\n"
"  sqlite3*,\n"
"  const void *zName,\n"
"  int eTextRep,\n"
"  void *pArg,\n"
"  int(*xCompare)(void*,int,const void*,int,const void*)\n"
");\n"
